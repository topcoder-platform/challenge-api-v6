{
    "name": "prisma-migration-tool",
    "version": "1.0.0",
    "description": "Tool for migrating JSON data to PostgreSQL using Prisma",
    "main": "src/index.js",
    "scripts": {
        "db:up": "docker compose -f local/docker-compose.yml up -d",
        "db:down": "docker compose -f local/docker-compose.yml down",
        "db:reset": "docker compose -f local/docker-compose.yml down -v && docker compose -f local/docker-compose.yml up -d",
        "migrate": "node src/index.js",
        "migrate:reset": "npx prisma migrate reset -f && npm run migrate",
        "test": "jest",
        "test:watch": "jest --watch",
        "test:coverage": "jest --coverage",
        "migrate:test": "npm run migrate && npm run test",
        "migrate:reset:test": "npm run migrate:reset && npm run test",
        "lint": "eslint .",
        "lint:fix": "eslint . --fix"
    },
    "dependencies": {
        "@prisma/client": "~7.1.0",
        "dotenv": "^17.2.3",
        "event-stream": "^4.0.1",
        "JSONStream": "^1.3.5",
        "uuid": "^13.0.0"
    },
    "devDependencies": {
        "@eslint/js": "^9.39.1",
        "eslint": "^9.39.1",
        "eslint-plugin-jest": "^29.2.1",
        "globals": "^16.5.0",
        "jest": "^30.2.0",
        "prisma": "~7.1.0"
    },
    "volta": {
        "node": "18.19.0",
        "yarn": "1.22.19"
    },
   "overrides": {
    "glob": "10.4.2" 
  }
}
